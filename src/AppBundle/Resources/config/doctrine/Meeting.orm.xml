<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity repository-class="AppBundle\Repository\UserRepository" name="AppBundle\Entity\Meeting" table="skygate_meetings">
        <id name="meeting_id" type="integer" column="meeting_id">
            <generator strategy="AUTO"/>
        </id>
        <field name="meeting_name" type="string" column="meeting_name" length="128" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="date" type="date" column="date" length="128" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="time" type="time" column="time" length="0" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="place" type="string" column="place" length="128" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="objective" type="text" column="objective" length="128" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="isAttending" type="string" column="isAttending" length="128" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
<!--        <field name="agenda" type="collection" column="agenda" length="10248" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>-->
        <field name="isComplete" type="boolean" column="isComplete" length="1" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="file" type="string" column="file" length="128" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="agenda" type="string" column="agenda" length="128" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
    </entity>
</doctrine-mapping>
